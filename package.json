{
  "name": "bright-table",
  "author": "Saroj Regmi",
  "license": "MIT",
  "version": "0.0.1",
  "description": "A React table library for bright office system.",
  "type": "module",
  "main": "./dist/src/index.js",
  "types":"./dist/types/index.d.ts",
  "files": [ "dist/" ],
  "scripts": {
    "format": "prettier --write \"{src,test}/**/*.{tsx,ts,js}\"",
    "format:check": "prettier --list-different \"{src,test}/**/*.{tsx,ts,js}\"",
    "watch:lib": "vite build --watch",
    "watch:types": "tsc --watch --emitDeclarationOnly",
    "watch": "run-p  watch:lib watch:types",
    "build": "vite build",
    "build:types": "npx tsc --emitDeclarationOnly",
    "build:publish":"bun run build && bun run build:types",
    "build:docs": "",
    "postbuild": "mocha test/build.test.cjs",
    "tdd": "cross-env NODE_ENV=test karma start",
    "lint:ts": "eslint src/**/*.{ts,tsx}",
    "lint": "bun run lint:ts",
    "test": "bun run lint && cross-env NODE_ENV=test karma start --single-run && bun run build && bun run test:types",
    "test:types": "tsc -p tsconfig.test.json --noEmit",
    "clean": "rm -rf node_modules/gh-pages/.cache",
    "coveralls": "cat ./coverage/lcov/lcov.info | ./node_modules/.bin/coveralls",
    "prepublishOnly": "bun run build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@juggle/resize-observer": "^3.3.1",
    "@rsuite/icons": "^1.0.0",
    "classnames": "^2.3.1",
    "clsx": "^2.1.1",
    "dom-lib": "^3.3.1",
    "lodash": "^4.17.21",
    "react-is": "^17.0.2",
    "tailwind-merge": "^2.5.4"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "fs-extra": "^11.2.0",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@faker-js/faker": "^7.6.0",
    "@testing-library/react": "^13.4.0",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.14.165",
    "@types/prop-types": "^15.7.1",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-is": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "autoprefixer": "^10.4.20",
    "chai": "^4.1.0",
    "conventional-changelog-cli": "^2.1.1",
    "coveralls": "^3.1.0",
    "cross-env": "^5.2.1",
    "css-loader": "^0.23.1",
    "cssnano": "^4.1.10",
    "cz-conventional-changelog": "^3.3.0",
    "del": "^5.1.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^0.10.1",
    "gh-pages": "^0.12.0",
    "karma": "^6.4.0",
    "karma-chai-dom": "^1.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage": "^2.0.3",
    "karma-firefox-launcher": "^2.1.1",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon-chai": "^2.0.2",
    "karma-sourcemap-loader": "^0.3.8",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "mini-css-extract-plugin": "^2.3.0",
    "mocha": "^9.0.2",
    "prettier": "^2.3.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-code-view": "^2.2.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "rsuite": "^5.69.0",
    "sinon": "^11.1.2",
    "sinon-chai": "^3.7.0",
    "style-loader": "^0.13.1",
    "tailwindcss": "^3.4.14",
    "ts-expect": "^1.3.0",
    "typescript": "^5.2.2",
    "url-loader": "^0.5.7",
    "vite": "^5.4.9"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
